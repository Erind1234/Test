// Modify your form submission code
$("button[type='submit']").click(function (e) {
    e.preventDefault();

    if ($(this).hasClass("btn-primary")) {
        // Get the value from the searchBox
        var searchValue = $("#searchBox").val();

        // Set the QRCODEVAL input field to the searchValue
        $("input[name='QRCODEVAL']").val(searchValue);

        // Submit the form
        $("form").submit();
    }
});
